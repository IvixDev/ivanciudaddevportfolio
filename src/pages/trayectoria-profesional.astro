---
import { seoMapper } from "../helpers/seo.helper";
import Layout from "../layouts/Layout.astro";
import CareerStepper from "../components/CareerStepper.astro";

const title = "Iván Ciudad Espinar - Mi Trayectoria Profesional y CV";
const description =
  "Explora la carrera profesional de Iván Ciudad Espinar, Software Developer e Ingeniero Telemático. Consulta mi CV y descubre mis experiencias, habilidades y logros en el desarrollo de software.";
const url = "https://www.ivanciudad.com/trayectoria-profesional";
const image = "https://www.ivanciudad.com/trayectoria-profesional.webp";

const infoSeo = seoMapper(title, description, url, image);
---

<Layout infoSeo={infoSeo}>
  <CareerStepper />
</Layout>
